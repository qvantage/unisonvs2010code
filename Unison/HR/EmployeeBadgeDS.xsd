<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="EmployeeBadgeDS" targetNamespace="http://tempuri.org/EmployeeBadgeDS.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/EmployeeBadgeDS.xsd" xmlns:mstns="http://tempuri.org/EmployeeBadgeDS.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="EmployeeBadgeDS" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="EmployeeBadge">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Company" type="xs:string" />
							<xs:element name="OfficeId" type="xs:int" />
							<xs:element name="Office" type="xs:string" />
							<xs:element name="EmployeeId" type="xs:int" />
							<xs:element name="FullName" type="xs:string" />
							<xs:element name="Status" type="xs:string" />
							<xs:element name="StatusDate" type="xs:dateTime" />
							<xs:element name="CreateDate" type="xs:dateTime" />
							<xs:element name="HireDate" type="xs:dateTime" />
							<xs:element name="DOB" type="xs:dateTime" />
							<xs:element name="DLN" type="xs:string" />
							<xs:element name="Photo" type="xs:base64Binary" />
							<xs:element name="AddressID" type="xs:int" />
							<xs:element name="Hair" type="xs:string" />
							<xs:element name="Eyes" type="xs:string" />
							<xs:element name="EmployeeHeight" type="xs:string" />
							<xs:element name="EmployeeWeight" type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>